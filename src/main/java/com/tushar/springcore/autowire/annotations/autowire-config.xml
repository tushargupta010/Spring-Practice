<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">
     
     <!-- default :- byType autowiring  -->          
     <context:annotation-config/>
     
     <bean class="com.tushar.springcore.autowire.annotations.Book" name="book123">
     	<property name="name" value="The Invisible Man" />
     </bean>
     
     <bean class="com.tushar.springcore.autowire.annotations.Student" name="student" />
     
     
</beans>